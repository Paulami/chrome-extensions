{
  "manifest_version": 2,
  "name": "Robinhood grayscale",
  "version": "0.2",
  "content_scripts": [
    {
      "matches": ["*://robinhood.com/*"],
      "js": ["content.js"],
      "run_at": "document_end"
    }
  ],
  "browser_action": {
    "default_icon": "icon.png"
  },
  "background": {
    "scripts": ["background.js"]
  }
}
